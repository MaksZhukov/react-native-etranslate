{
  "expo": {
    "name": "react-native-etranslate",
    "slug": "react-native-etranslate",
    "version": "1.0.0",
    "orientation": "portrait",
	"icon": "./assets/icon.png",
	"scheme": "e-translate",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "",
    "githubUrl": "https://github.com/MaksZhukov/react-native-etranslate",
    "android": {
	  "useNextNotificationsApi": true,
      "package": "com.maksimzhukau.reactnativeetranslate"
    }
  }
}
